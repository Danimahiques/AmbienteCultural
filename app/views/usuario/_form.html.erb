

<%= form_tag usuarios_path do %>
 <div class="form-group">
    <%= label_tag :nombre  %>
    <%= text_field_tag :nombre %> 
 </div>
 <br>
 <div class="form-group">
    <%= label_tag :apellido  %>
    <%= text_field_tag :apellido  %>
 </div>
 <br>
  <div class="form-group">
    <%= label_tag :email  %>
    <%= text_field_tag :email  %>
 </div>
 <br>
 <div class="form-group">
    <%= label_tag :nickname  %>
    <%= text_field_tag :nickname  %>
 </div>
 <br>
 <div class="form-group">
    <%= label_tag :password  %>
    <%= text_field_tag :password  %>
 </div>
 <br>
    <%= submit_tag %>

<% end %>    

